{
  "name": "dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "Full stack MERN application",
  "main": "bin/www",
  "scripts": {
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "npm start --prefix client",
    "client-install": "npm install --prefix client",
    "install-app": "npm install && npm install client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "start-app": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Filip Lipi≈Ñski",
  "license": "ISC",
  "dependencies": {
    "atob": "^2.1.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.3",
    "morgan": "~1.9.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^1.19.3"
  }
}
